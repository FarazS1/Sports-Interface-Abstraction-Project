public abstract class Abstract_Turf<A, B, C> implements Sport<A, B, C>
{
    private A rules;
    private B topPlayers;
    
    Abstract_Turf(A r, B tP){
        rules = r;
        topPlayers = tP;
    }
    
    public void setRules(A r){rules = r;}
    public A getRules(){return this.rules;}
    public void setTopPlayers(B tP){topPlayers = tP;}
    public B getTopPlayers(){return this.toPlayers;}
}
